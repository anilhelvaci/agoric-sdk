{
  "name": "@agoric/swingset-runner",
  "version": "0.22.0",
  "private": true,
  "description": "Application to launch SwingSet instances for development and testing",
  "type": "module",
  "main": "./src/main.js",
  "repository": "https://github.com/Agoric/agoric-sdk",
  "author": "Agoric",
  "license": "Apache-2.0",
  "scripts": {
    "build": "exit 0",
    "test": "ava",
    "test:c8": "c8 $C8_OPTIONS ava --config=ava-nesm.config.js",
    "test:xs": "exit 0",
    "lint-fix": "yarn lint:eslint --fix",
    "lint": "run-s --continue-on-error lint:*",
    "lint:eslint": "eslint .",
    "ci:autobench": "./autobench.js"
  },
  "dependencies": {
    "@agoric/assert": "^0.6.0",
    "@agoric/ertp": "^0.16.0",
    "@agoric/stat-logger": "^0.4.28",
    "@agoric/store": "^0.9.0",
    "@agoric/swing-store": "^0.9.0",
    "@agoric/swingset-vat": "^0.31.0",
    "@agoric/telemetry": "^0.5.0",
    "@agoric/vat-data": "^0.5.0",
    "@agoric/zoe": "^0.26.0",
    "@endo/bundle-source": "^2.5.0",
    "@endo/eventual-send": "^0.17.1",
    "@endo/init": "^0.5.55",
    "@endo/marshal": "^0.8.4",
    "@endo/nat": "^4.1.26",
    "expose-gc": "^1.0.0",
    "n-readlines": "^1.0.1",
    "yargs": "^16.1.0"
  },
  "devDependencies": {
    "ava": "^5.2.0",
    "c8": "^7.13.0",
    "import-meta-resolve": "^2.2.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "timeout": "2m"
  }
}
