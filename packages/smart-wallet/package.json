  "devDependencies": {
    "@agoric/cosmic-proto": "^0.3.0",
    "@endo/captp": "^3.1.0",
    "@endo/init": "^0.5.55",
    "ava": "^5.2.0"
  },
  "dependencies": {
    "@agoric/assert": "^0.6.0",
    "@agoric/casting": "^0.4.0",
    "@agoric/ertp": "^0.16.0",
    "@agoric/internal": "^0.3.0",
    "@agoric/notifier": "^0.6.0",
    "@agoric/store": "^0.9.0",
    "@agoric/swingset-vat": "^0.31.0",
    "@agoric/vat-data": "^0.5.0",
    "@agoric/vats": "^0.14.0",
    "@agoric/zoe": "^0.26.0",
    "@endo/eventual-send": "^0.17.1",
    "@endo/far": "^0.2.17",
    "@endo/marshal": "^0.8.4",
    "@endo/nat": "^4.1.26",
    "@endo/promise-kit": "^0.2.55"
  },
  "files": [
    "src/"
  ],
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Agoric/agoric"
  },
  "author": "Agoric",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Agoric/agoric/issues"
  },
  "homepage": "https://github.com/Agoric/agoric#readme",
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "timeout": "2m"
  },
  "publishConfig": {
    "access": "public"
  }
}
